#include<stdio.h>
#define TRUE 1
#define FALSE 0
#define SQ(x) (x)*(x)
int main()
{
    int num;
    int again=1;
    printf("如果值< 50 程序停止\n");
    while(again)
    {
        printf("\n輸入字：");
        scanf("%d",&num);
        printf("字的平方 %d \n",SQ(num));
        if(num>=50)
            again=TRUE;
        else
            again=FALSE;
    }
    return 0;
}
