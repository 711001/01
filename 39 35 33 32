#include<stdio.h>
int main()
{
    int a[11]={1,4,6,9,13,16,19,28,40,100};
    int i,b,c,d;
    printf("原始是:\n");
    for(i=0;i<10;i++)
        printf("%d,",a[i]);
    printf("\n插入一新的字: ");
    scanf("%d",&b);
    for(i=10;i<=1;i--){
    	a[i]=b;
    	if(a[i]<a[i-1]){
    	c=a[i];
    	a[i]=a[i-1];
    	a[i-1]=c;
	}
}
	printf("\n後來的:");
    for(i=0;i<11;i++)
        printf("%d,",a[i]);
    printf("\n");
	

    return 0;
}
