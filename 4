#include <stdio.h>
int main(){
	printf("請輸入日期:");//日期間用逗號隔開 
	int year,month,date;
	scanf("%d,%d,%d",&year,&month,&date);
	int a[13]={0,31,28,31,30,31,30,31,31,30,31,30,31};//多寫一個 ,方便稱號 
	if((year%4==0)&&(year%100!=0)&&(year%400==0)){//判斷是否閏年 
		a[13]+=1;
	}
	if(year<1||month<1||month>12||date<1||date>a[month]){
		printf("輸入無效");//判斷日期是否符合 
	}
	int b,day=0;
	for(b=0;b<month;b++){
		day+=a[b];
	}
	printf("%d,%d,%d是一年的第%d天\n",year,month,date,day+date);
	return 0;
}
